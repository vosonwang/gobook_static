    "iview": "^2.8.0",
    "mavon-editor": "^2.4.3",
    "messageformat": "^1.1.0",
    "moment": "^2.19.3",
    "utf8": "^3.0.0",
    "vue": "^2.5.8",
    "vue-i18n": "^7.3.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
